<%= render UI::Breadcrumb::Component.new do |breadcrumb| %>
  <% breadcrumb.with_item(first: true) do |item| %>
    <% item.with_link(href: "#").with_content("Home") %>
  <% end %>

  <%= breadcrumb.with_item do |item| %>
    <% item.with_separator %>
    <% item.with_link(href: "#").with_content("Library") %>
  <% end %>

  <% breadcrumb.with_item do |item| %>
    <% item.with_separator %>
    <% item.with_link(href: "#", active: true).with_content("Data") %>
  <% end %>
<% end %>

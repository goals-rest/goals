<%= render UI::ActivityCalendar::Component.new(calendar_date_range) do |activity_calendar| %>
  <% progress_calendar.days.each do |day| %>
    <% activity_calendar.with_day(
      grade: day.grade, 
      href: profile_goal_progress_changes_path(user.username, date: day.date)
    ) do %>
      <%= tooltip_content_for(day) %>
    <% end %>
  <% end %>
<% end %>
